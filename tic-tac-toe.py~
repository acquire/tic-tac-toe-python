a1 = "a1"
a2 = "a2"
a3 = "a3"
b1 = "b1"
b2 = "b2"
b3 = "b3"
c1 = "c1"
c2 = "c2"
c3 = "c3"
game_over = False;

while(game_over == False):
    print("{} | {} | {}".format(a1,a2,a3))
    print("===+====+===")
    print("{} | {} | {}".format(b1,b2,b3))
    print("===+====+===")
    print("{} | {} | {}".format(c1,c2,c3))
    
    print("x turn")
    print("location:")
    input = input()
    if input == "a1":
        a1 = "x"


            


